
@{
    ViewData["Title"] = "BlogDetails";
    Layout = "~/Views/UILayout/Index.cshtml";
}

@await Component.InvokeAsync("_BlogDetailMainCoverComponentPartial")

<section class="ftco-section ftco-degree-bg">
    <div class="container">
        <div class="row">

            @await Component.InvokeAsync("_BlogDetailsMainComponentPartial", new { id = ViewBag.blogid })

            <div class="col-md-4 sidebar ftco-animate">

                @await Component.InvokeAsync("_BlogDetailSearchBoxComponentPartial")

                @await Component.InvokeAsync("_BlogDetailCategoriesComponentPartial")

                @await Component.InvokeAsync("_BlogDetailRecentBlogComponentPartial")

                @await Component.InvokeAsync("_BlogDetailTagCloudComponentPartial")

                @await Component.InvokeAsync("_BlogDetailParagraphComponentPartial")

            </div>
        </div>
    </div>
</section>

